{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost; Port=5432; User Id=postgres; Password=password; Database=Modern;"
  },
  "Sentry": {
    "Dsn": "https://<key>@o0.ingest.sentry.io/0",
    "TracesSampleRate": 0.5,
    "SendDefaultPii": false, // scrub usernames, IPs
    "MinimumBreadcrumbLevel": "Information",
    "MinimumEventLevel": "Error"
  }

}
